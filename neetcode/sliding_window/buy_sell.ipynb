   "source": [
    "prices = [7,6,4,3,1]\n",
    "def test():\n",
    "    s,e = 0,1\n",
    "    res = 0\n",
    "    while e < len(prices):\n",
    "        res = max(res, prices[e] - prices[s])\n",
    "        if prices[s] > prices[e]:\n",
    "            s = e\n",
    "        e += 1\n",
    "    return res\n",
    "\n",
    "\n",
    "print(test())"
   ]
